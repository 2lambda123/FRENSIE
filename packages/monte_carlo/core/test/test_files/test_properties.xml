<ParameterList name="Properties">

  <ParameterList name="General Properties">
    <Parameter name="Mode" type="string" value="Neutron-Photon"/>
    <Parameter name="Histories" type="unsigned int" value="10"/>
    <Parameter name="Surface Flux Angle Cosine Cutoff" type="double" value="0.1"/>
    <Parameter name="Implicit Capture" type="bool" value="true"/>
    <Parameter name="Warnings" type="bool" value="false"/>
    <Parameter name="Ideal Batches Per Processor" type="unsigned int" value="25"/>
  </ParameterList>

  <ParameterList name="Neutron Properties">
    <Parameter name="Free Gas Threshold" type="double" value="600.0"/>
    <Parameter name="Min Neutron Energy" type="double" value="1e-2"/>
    <Parameter name="Max Neutron Energy" type="double" value="10.0"/>
    <Parameter name="Unresolved Resonance Probability Table" type="bool" value="false"/>
  </ParameterList>

  <ParameterList name="Photon Properties">
    <Parameter name="Min Photon Energy" type="double" value="1e-2"/>
    <Parameter name="Max Photon Energy" type="double" value="10.0"/>
    <Parameter name="Kahn Sampling Cutoff Energy" type="double" value="2.5"/>
    <Parameter name="Photon Hash Grid Bins" type="unsigned int" value="500"/>
    <Parameter name="Incoherent Photon Scattering Model" type="string" value="DHP Doppler Broadened Hybrid Model"/>
    <Parameter name="Photon Atomic Relaxation" type="bool" value="false"/>
    <Parameter name="Detailed Pair Production" type="bool" value="true"/>
    <Parameter name="Photonuclear Interaction" type="bool" value="true"/>
  </ParameterList>

  <ParameterList name="Adjoint Photon Properties">
    <Parameter name="Min Adjoint Photon Energy" type="double" value="1e-2"/>
    <Parameter name="Max Adjoint Photon Energy" type="double" value="10.0"/>
    <Parameter name="Adjoint Photon Hash Grid Bins" type="unsigned int" value="500"/>
    <Parameter name="Incoherent Adjoint Photon Scattering Model" type="string" value="Klein-Nishina Adjoint Model"/>
    <Parameter name="Critical Adjoint Photon Line Energies" type="Array" value="{0.1, 1.0, 10.0}"/>
  </ParameterList>

  <ParameterList name="Electron Properties">
    <Parameter name="Min Electron Energy" type="double" value="1e-2"/>
    <Parameter name="Max Electron Energy" type="double" value="10.0"/>
    <Parameter name="Electron Atomic Relaxation" type="bool" value="false"/>
    <Parameter name="Electron Elastic" type="bool" value="false"/>
    <Parameter name="Electron Electroionization" type="bool" value="false"/>
    <Parameter name="Electron Bremsstrahlung" type="bool" value="false"/>
    <Parameter name="Electron Atomic Excitation" type="bool" value="false"/>
    <Parameter name="Electron LinLinLog Interpolation" type="bool" value="false"/>
    <Parameter name="Electron Weighted Interpolation" type="bool" value="false"/>
    <Parameter name="Bremsstrahlung Angular Distribution" type="string" value="Dipole"/>
    <Parameter name="Elastic Cutoff Angle Cosine" type="double" value="0.9"/>
  </ParameterList>

</ParameterList>
