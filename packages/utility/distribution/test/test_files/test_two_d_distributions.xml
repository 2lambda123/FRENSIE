<ParameterList name="Two-D Distributions">

  <ParameterList name="One-D Distributions">
    <Parameter name="D0" type="Tabular LogLog Distribution"
               value="{{0.001, 2l, 1.0},{100,10,1,0.1}}"/>
    <Parameter name="D1" type="Tabular LogLog Distribution"
               value="{{0.01, 2l, 1.0},{10,1,0.1,0.01}}"/>
    <Parameter name="D2" type="Histogram Distribution"
               value="{{0.0, 3i, 1.0},{1.0, 2.0, 2.0, 1.0}}"/>
    <Parameter name="D3" type="Histogram Distribution"
               value="{{0.1, 3i, 0.9},{1.0, 2.0, 3.0, 4.0}}"/>
    <Parameter name="D4" type="Power 1 Distribution" value="{2,0,1}"/>
    <Parameter name="D5" type="Power 2 Distribution" value="{3,0,1}"/>
    <Parameter name="D6" type="Uniform Distribution" value="{0.0, 1.0, 2}"/>
    <Parameter name="D7" type="Uniform Distribution" value="{0.1, 0.9, 1.5}"/>
    <Parameter name="D8" type="Normal Distribution"
               value="{0.5, 0.25, 0, 1}"/>
  </ParameterList>

  <ParameterList name="Histogram Partially-Tabular Two-D Distribution">
    <Parameter name="Type" type="string" value="Histogram Distribution"/>
    <Parameter name="Primary Grid" type="Array" value="{-1.0, 0.0, 1.0}"/>
    <Parameter name="Secondary Distributions" type="Array" value="{D0, D4}"/>
  </ParameterList>

  <ParameterList name="Histogram Fully-Tabular Two-D Distribution">
    <Parameter name="Type" type="string" value="Histogram Distribution"/>
    <Parameter name="Combined Grid Distributions" type="Array"
               value="{{-1.0,  D0},
                       {-0.25, D1},
                       { -0.5, D6},
                       { 0.5,  D1},
                       { 0.75, D0},
                       { 1.0, *}"/>
  </ParameterList>

  <ParameterList name="Interpolated Partially-Tabular Two-D Distribution">
    <Parameter name="Type" type="string"
               value="Interpolated LinLinLin Distribution"/>
    <Parameter name="Combined Grid Distributions" type="Array"
               value="{{0.0,  D0},
                       {0.25, D1},
                       {0.5,  D2},
                       {0.75, D3},
                       {1.0,  D4},
                       {1.5,  D5},
                       {2.0,  D6},
                       {2.5,  D7},
                       {3.0,  D8}}"/>
  </ParameterList>

  <ParameterList name="Interpolated Fully-Tabular Two-D Distribution">

    <Parameter name="Type" type="string"
               value="Interpolated LinLinLog Distribution"/>
    <Parameter name="Primary Grid" type="Array" value="{0.0001, 4l, 10.0}"/>
    <Parameter name="Secondary Distributions" type="Array"
               value="{D0,D1,D2,D3,D6,D7}"/>

  </ParameterList>
  
</ParameterList>
