<ParameterList name="Spatial Distributions">

  <ParameterList name="Cartesian Distribution Example">
    <Parameter name="X Distribution" type="Delta Distribution" value="{0}"/>
    <Parameter name="Y Distribution" type="Uniform Distribution"
	       value="{-1.0, 1.0, 2}"/>
    <Parameter name="Z Distribution" type="Normal Distribution"
	       value="{0.0, 1.0}"/>
  </ParameterList>

  <ParameterList name="Cylindrical Distribution Example">
    <Parameter name="R Distribution" type="Power 1 Distribution"
	       value="{1.0, 0.0, 1.0}"/>
    <Parameter name="Theta Distribution" type="Uniform Distribution"
	       value="{0,6.283185307179586,1}"/>
    <Parameter name="Axis Distribution" type="Uniform Distribution"
	       value="{-10,10,1}"/>
    <Parameter name="Center X Position" type="double" value="0.0"/>
    <Parameter name="Center Y Position" type="double" value="0.0"/>
    <Parameter name="Center Z Position" type="double" value="0.0"/>
    <Parameter name="Axis" type="string" value="Z"/>
  </ParameterList>

  <ParameterList name="Spherical Distribution Example">
    <Parameter name="R Distribution" type="Power 2 Distribution"
	       value="{1.0, 0.0, 1.0}"/>
    <Parameter name="Theta Distribution" type="Uniform Distribution"
	       value="{0,6.283185307179586,1}"/>
    <Parameter name="Mu Distribution" type="Uniform Distribution"
	       value="{-1,1,1}"/>
    <Parameter name="Center X Position" type="double" value="0.0"/>
    <Parameter name="Center Y Position" type="double" value="0.0"/>
    <Parameter name="Center Z Position" type="double" value="0.0"/>
    <Parameter name="Axis" type="string" value="Y"/>
  </ParameterList>

  <ParameterList name="Point Distribution Example">
    <Parameter name="Position" type="Array(double)" value="{1.0,1.0,1.0}"/>
  </ParameterList>

</ParameterList>
